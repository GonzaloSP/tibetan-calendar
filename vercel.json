{
  "version": 2,
  "cleanUrls": true,
  "trailingSlash": true,
  "buildCommand": "cd app && npm run build:social",
  "outputDirectory": "app/dist",
  "routes": [
    { "handle": "filesystem" },
    {
      "src": "/fecha/(\\d{4}-\\d{2}-\\d{2})/?$",
      "dest": "/fecha/$1/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
